<!-- DataTales Example -->
<div
  class="tab-pane fade show active"
  id="show-image"
  role="tabpanel"
  aria-labelledby="home-tab"
>
  <div class="row">
    <% items.imageId.forEach(item => { %>
    <div class="col-md-4">
      <div class="card shadow mb-4 mt-2">
        <div class="card-body">
          <img
            src="http://just-staycation.herokuapp.com/<%= item.imageUrl %> "
            alt="images"
            style="width: 100%; display: block"
          />
        </div>
      </div>
    </div>
    <% }) %>
  </div>
  <a href="/admin/item" class="btn btn-primary">Back</a>
</div>
